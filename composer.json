{
    "name": "indiehd/audio-manipulator",
    "type": "library",
    "description": "Library for manipulating audio files (validating, transcoding, and tagging)",
    "keywords": ["audio","flac","mp3","wav","alac"],
    "homepage": "https://github.com/indiehd/audio-manipulator",
    "license": "MIT",
    "authors": [
        {
            "name": "indieHD, LLC",
            "email": "webmaster@indiehd.com",
            "homepage": "https://indiehd.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/indiehd/audio-manipulator/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "symfony/process": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "james-heinrich/getid3": "1.9.23",
        "monolog/monolog": "^2.0",
        "league/flysystem": "^3.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "IndieHD\\AudioManipulator\\": "src/",
            "IndieHD\\AudioManipulator\\Tests\\": "tests/"
        },
        "files": ["src/Bootstrap.php"]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpro/grumphp": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
